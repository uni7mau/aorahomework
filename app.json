{
  "expo": {
    "name": "Aora",
    "slug": "aorahomework",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aora",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uni7mau.aora",
      "packages": "com.jsm.aorahomework"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3e3cc9ab-233e-4b4f-b9e0-776bd9905978"
      }
    },
    "owner": "uni7mau",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3e3cc9ab-233e-4b4f-b9e0-776bd9905978"
    }
  },
  "plugins": [
    "expo-updates",
    {
      "username": "uni7mau"
    }
  ]
}
